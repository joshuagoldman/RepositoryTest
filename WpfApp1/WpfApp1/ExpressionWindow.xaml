                <TextBox Name="VariableNine" Grid.Row="1" Visibility="{Binding Path= VariableOne.Visibility, Mode=TwoWay}"   Background="{Binding Path= Variable.ExpressionModel.VariableOne.Background, Mode=TwoWay}" Foreground="{Binding Path= Variable.ExpressionModel.VariableOne.Foreground, Mode=TwoWay}"></TextBox>
            </Grid>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="100"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="30"/>
                    <RowDefinition Height="30"/>
                </Grid.RowDefinitions>
                <TextBlock Name="VariableTenLabel" Grid.Row="0" Text="Variable" TextWrapping="{Binding Path= StandardLabelAppearance1.TextWrapping, Mode=TwoWay}" Background="{Binding Path= StandardLabelAppearance1.Background}" TextAlignment="{Binding Path= StandardLabelAppearance1.TextAlignment, Mode=TwoWay}" FontSize="{Binding Path= StandardLabelAppearance1.FontSize, Mode=TwoWay}"></TextBlock>
                <TextBox Name="VariableTen"  Grid.Row="1" Visibility="{Binding Path= VariableOne.Visibility, Mode=TwoWay}"   Background="{Binding Path= Variable.ExpressionModel.VariableOne.Background, Mode=TwoWay}" Foreground="{Binding Path= Variable.ExpressionModel.VariableOne.Foreground, Mode=TwoWay}"></TextBox>
            </Grid>
        </WrapPanel>
    </Grid>
</Window>
